@model IEnumerable<Core.Model.Inscricao>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            ID Inscrito
        </th>
        <th>
            Nome
        </th>
        <th>
            Instituição
        </th>
        <th>
            Email
        </th>
        <th>
            Telefone
        </th>
        <th>
            Curso
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Id
        </td>
        <td>
            @item.nome
        </td>
        <td>
            @item.instituicao
        </td>
        <td>
            @item.email
        </td>
        <td>
            @item.telefone
        </td>
        <td>
            @item.curso
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

